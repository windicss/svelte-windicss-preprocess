{
  "name": "svelte-windicss-preprocess",
  "version": "3.1.2",
  "description": "A Svelte Preprocessor to compile tailwindcss at build time based on windicss compiler.",
  "main": "index.js",
  "types": "index.d.ts",
  "browser": "browser.js",
  "scripts": {
    "dev": "rollup -cw",
    "tsd": "tsc -p ./src --emitDeclarationOnly",
    "test": "jest",
    "build": "rollup -c && npm run tsd",
    "build:prod": "cross-env PUBLISH=true rollup -c && npm run tsd",
    "release": "bumpp --tag --commit --push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voorjaar/svelte-windicss-preprocess.git"
  },
  "keywords": [
    "svelte",
    "tailwindcss",
    "css",
    "compiler"
  ],
  "author": "Veritas Raven",
  "license": "MIT",
  "homepage": "https://github.com/voorjaar/svelte-windicss-preprocess",
  "dependencies": {
    "chalk": "^4.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.35.0",
    "windicss": "^2.5.4",
    "windicss-runtime-dom": "^0.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/diff": "^5.0.0",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.21",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.35",
    "bumpp": "^6.0.6",
    "cross-env": "^7.0.3",
    "diff": "^5.0.0",
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "js-yaml": "^4.0.0",
    "rollup": "^2.42.4",
    "rollup-plugin-terser": "^7.0.2",
    "sucrase": "^3.17.1",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
