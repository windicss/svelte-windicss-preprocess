{
  "name": "svelte-windicss-preprocess",
  "description": "A custom svelte preprocessor to use windicss with svelte.",
  "version": "4.5.0",
  "author": "Veritas Raven",
  "bugs": {
    "url": "https://github.com/windicss/svelte-windicss-preprocess/issues"
  },
  "dependencies": {
    "@unocss/core": "0.37.2",
    "@unocss/preset-attributify": "0.37.2",
    "@unocss/preset-icons": "0.37.2",
    "@unocss/preset-typography": "0.37.2",
    "@unocss/preset-wind": "0.37.2",
    "css-tree": "2.1.0",
    "fast-glob": "3.2.11",
    "magic-string": "0.26.2",
    "unconfig": "0.3.4"
  },
  "devDependencies": {
    "@alexanderniebuhr/style": "1.3.0",
    "@iconify/types": "1.1.0",
    "@nbhr/utils": "0.0.30",
    "@types/css-tree": "1.0.7",
    "@types/node": "17.0.38",
    "bumpp": "7.1.1",
    "cross-env": "7.0.3",
    "rimraf": "3.0.2",
    "svelte": "3.48.0",
    "tsm": "2.2.1",
    "tsup": "6.0.1",
    "typescript": "4.7.2",
    "windicss": "3.5.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://windicss.org/",
  "keywords": [
    "compiler",
    "css",
    "preprocess",
    "svelte",
    "tailwindcss",
    "unocss",
    "windicss"
  ],
  "license": "MIT",
  "maintainers": [
    {
      "name": "Alexander Niebuhr",
      "email": "tech@nbhr.io"
    }
  ],
  "repository": "github:windicss/svelte-windicss-preprocess",
  "scripts": {
    "build": "tsup ./src/index.ts --dts --format esm --no-splitting",
    "build:clean": "rimraf dist && npm run build",
    "dev": "npm run build --watch",
    "lint": "eslint src",
    "lint:tests": "eslint tests --ext .ts",
    "release": "npm run test && npm run build:clean && bumpp --tag --commit --push",
    "test": "uvu -r sucrase/register tests",
    "test:generate": "tsm generate.ts"
  },
  "types": "dist/index.d.ts"
}