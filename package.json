{
  "name": "svelte-windicss-preprocess",
  "description": "A Svelte Preprocessor to compile tailwindcss at build time based on windicss compiler.",
  "version": "4.4.0-alpha.4",
  "author": "Veritas Raven",
  "dependencies": {
    "@iconify/json": "2.1.35",
    "@unocss/core": "0.32.8",
    "@unocss/preset-attributify": "0.32.8",
    "@unocss/preset-icons": "0.32.8",
    "@unocss/preset-typography": "0.32.8",
    "@unocss/preset-wind": "0.32.8",
    "css-tree": "2.1.0",
    "fast-glob": "3.2.11",
    "magic-string": "0.26.1",
    "unconfig": "0.3.3"
  },
  "devDependencies": {
    "@alexanderniebuhr/style": "1.2.0",
    "@nbhr/utils": "0.0.30",
    "@types/css-tree": "1.0.7",
    "@types/node": "17.0.31",
    "bumpp": "7.1.1",
    "cross-env": "7.0.3",
    "nanocolors": "0.2.13",
    "rimraf": "3.0.2",
    "svelte": "3.48.0",
    "tsm": "2.2.1",
    "tsup": "5.11.13",
    "typescript": "4.6.4",
    "windicss": "3.5.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://windicss.org/",
  "keywords": [
    "compiler",
    "css",
    "preprocess",
    "svelte",
    "tailwindcss",
    "windicss"
  ],
  "license": "MIT",
  "maintainers": [
    {
      "name": "Alexander Niebuhr",
      "email": "tech@nbhr.io"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/windicss/svelte-windicss-preprocess.git"
  },
  "scripts": {
    "build": "tsup ./src/index.ts --dts --format esm --no-splitting",
    "build:clean": "rimraf dist && npm run build",
    "dev": "npm run build --watch",
    "lint": "eslint src",
    "lint:tests": "eslint tests --ext .ts",
    "release": "npm run test && npm run build:clean && bumpp --tag --commit --push",
    "test": "uvu -r sucrase/register tests",
    "test:generate": "tsm generate.ts"
  },
  "types": "dist/index.d.ts",
  "volta": {
    "node": "17.6.0"
  }
}
