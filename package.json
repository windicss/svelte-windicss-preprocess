{
  "name": "svelte-windicss-preprocess",
  "description": "A Svelte Preprocessor to compile tailwindcss at build time based on windicss compiler.",
  "version": "4.4.0-alpha.1",
  "author": "Veritas Raven",
  "dependencies": {
    "@iconify/json": "1.1.457",
    "css-tree": "2.0.0",
    "fast-glob": "3.2.11",
    "magic-string": "0.25.7",
    "unconfig": "0.2.2"
  },
  "devDependencies": {
    "@antfu/ni": "0.12.0",
    "@nbhr/utils": "0.0.29",
    "@types/css-tree": "1.0.7",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "@unocss/core": "0.22.3",
    "@unocss/preset-icons": "0.22.3",
    "@unocss/preset-wind": "0.22.3",
    "bumpp": "7.1.1",
    "cross-env": "7.0.3",
    "eslint": "8.7.0",
    "nanocolors": "0.2.13",
    "pnpm": "6.26.1",
    "rimraf": "3.0.2",
    "sucrase": "3.20.3",
    "svelte": "3.46.2",
    "tslib": "2.3.1",
    "tsm": "2.2.1",
    "tsup": "5.11.11",
    "typescript": "4.5.4",
    "uvu": "0.5.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://windicss.org/",
  "keywords": [
    "compiler",
    "css",
    "preprocess",
    "svelte",
    "tailwindcss",
    "windicss"
  ],
  "license": "MIT",
  "maintainers": [
    {
      "name": "Alexander Niebuhr",
      "email": "tech@nbhr.io"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/windicss/svelte-windicss-preprocess.git"
  },
  "scripts": {
    "build": "tsup ./src/index.ts --dts --format esm --no-splitting",
    "build:clean": "rimraf dist && nr build",
    "dev": "nr build --watch",
    "lint": "eslint src",
    "lint:tests": "eslint tests --ext .ts",
    "release": "nr test && nr build:clean && bumpp --tag --commit --push",
    "test": "uvu -r sucrase/register tests",
    "test:generate": "tsm generate.ts"
  },
  "types": "dist/index.d.ts",
  "volta": {
    "node": "17.3.1"
  }
}
