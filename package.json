{
  "name": "svelte-windicss-preprocess",
  "version": "4.0.3",
  "description": "A Svelte Preprocessor to compile tailwindcss at build time based on windicss compiler.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "test": "uvu -r sucrase/register tests",
    "build:clean": "rimraf dist && npm run build",
    "build": "tsup ./src/index.ts --dts --format esm,cjs",
    "release": "npm run build:clean && bumpp --tag --commit --push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/windicss/svelte-windicss-preprocess.git"
  },
  "keywords": [
    "svelte",
    "tailwindcss",
    "windicss",
    "preprocess",
    "css",
    "compiler"
  ],
  "author": "Veritas Raven",
  "license": "MIT",
  "homepage": "https://windicss.org/",
  "dependencies": {
    "prettier": "2.3.0",
    "prettier-plugin-svelte": "2.3.0",
    "windicss": "3.0.7",
    "windicss-runtime-dom": "0.1.1"
  },
  "devDependencies": {
    "@nbhr/utils": "0.0.27",
    "@types/diff": "5.0.0",
    "@types/fs-extra": "9.0.11",
    "@types/node": "15.0.3",
    "@types/prettier": "2.2.3",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "bumpp": "6.0.6",
    "chalk": "4.1.1",
    "cross-env": "7.0.3",
    "diff": "5.0.0",
    "eslint": "7.26.0",
    "rimraf": "3.0.2",
    "sucrase": "3.18.1",
    "svelte": "3.38.2",
    "tslib": "2.2.0",
    "tsup": "4.10.1",
    "typescript": "4.2.4",
    "uvu": "0.5.1"
  }
}
